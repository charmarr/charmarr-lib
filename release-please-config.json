    "vpn": {
      "release-type": "python",
      "package-name": "charmarr-lib-vpn",
      "changelog-path": "CHANGELOG.md",
      "extra-files": [
        {
          "type": "generic",
          "path": "src/charmarr_lib/vpn/_version.py"
        }
      ]
    },
    "testing": {
      "release-type": "python",
      "package-name": "charmarr-lib-testing",
      "changelog-path": "CHANGELOG.md",
      "extra-files": [
        {
          "type": "generic",
